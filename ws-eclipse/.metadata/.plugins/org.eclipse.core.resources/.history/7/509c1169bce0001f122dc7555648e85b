package ex01;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		double a = sc.nextDouble();
		double b = sc.nextDouble();
		double c = sc.nextDouble();
		double A, B, C;
		
		A = Math.pow(a, b);
		C = Math.pow(A, c);
		
		System.out.println(A, C);
		
		
		}
}
